ol.wait_micros(1300000)\n    move(MOVESTOP)\n\ndef on_sound_loud():\n    dance()\ninput.on_sound(DetectedSound.LOUD, on_sound_loud)\n\ndef on_button_pressed_ab():\n    move(MOVELEFT)\ninput.on_button_pressed(Button.AB, on_button_pressed_ab)\n\ndef on_button_pressed_b():\n    offEYE()\n    move(MOVESTOP)\ninput.on_button_pressed(Button.B, on_button_pressed_b)\n\ndef on_data_received():\n    global DISTANCE\n    DISTANCE = parse_float(serial.read_until(serial.delimiters(Delimiters.NEW_LINE)))\n    basic.show_number(DISTANCE)\n    if DISTANCE < 20:\n        spinMotorA(BRAKE)\n        spinMotorB(BRAKE)\nserial.on_data_received(serial.delimiters(Delimiters.NEW_LINE), on_data_received)\n\ndef move(action: number):\n    if DISTANCE > 20:\n        if action == MOVEFORWARD:\n            spinMotorA(FORWARD)\n            spinMotorB(FORWARD)\n        if action == MOVEBACKWARD:\n            spinMotorA(BACKWARD)\n            spinMotorB(BACKWARD)\n        if action == MOVESTOP:\n            spinMotorA(BRAKE)\n            spinMotorB(BRAKE)\n        if action == MOVELEFT:\n            spinMotorA(FORWARD)\n            spinMotorB(BACKWARD)\n        if action == MOVERIGHT:\n            spinMotorA(BACKWARD)\n            spinMotorB(FORWARD)\n    else:\n        spinMotorA(BRAKE)\n        spinMotorB(BRAKE)\ndef spinMotorA(num: number):\n    if num == FORWARD:\n        pins.digital_write_pin(DigitalPin.P0, 0)\n        pins.digital_write_pin(DigitalPin.P1, 1)\n    if num == BACKWARD:\n        pins.digital_write_pin(DigitalPin.P0, 1)\n        pins.digital_write_pin(DigitalPin.P1, 0)\n    if num == OFF:\n        pins.digital_write_pin(DigitalPin.P0, 0)\n        pins.digital_write_pin(DigitalPin.P1, 0)\n    if num == BRAKE:\n        pins.digital_write_pin(DigitalPin.P0, 1)\n        pins.digital_write_pin(DigitalPin.P1, 1)\nDISTANCE = 0\nMOVERIGHT = 0\nMOVELEFT = 0\nMOVEBACKWARD = 0\nMOVEFORWARD = 0\nMOVESTOP = 0\nBRAKE = 0\nFORWARD = 0\nBACKWARD = 0\nOFF = 0\ninput.set_sound_threshold(SoundThre